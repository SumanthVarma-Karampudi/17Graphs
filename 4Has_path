int main(){
    int n,e;
    cin>>n>>e;
    bool **edges=new *[n];
    for(int i=0;i<n;i++){
        edges[i]=new bool[n];
    }

    for(int i=0;i<n;i++){
        int start,end;
        cin>>a>>end;
        cin>>b>>end;
        edges[a][b]=true;
        edges[b][a]=true;
    }
    
    int start,end;
    cin>>start>>end;
    
    cout<<(HasPath(edges,v,start,end) ? "true": "false";
    
    for(int i=0;i<n;i++){
        delete [] edges[i];
    }
    delete [] edges;
}
